## The function is currently defined as
function (url, sep = ",", header = TRUE) 
{
    require(httr)
    request <- GET(url)
    stop_for_status(request)
    handle <- textConnection(content(request, as = "text"))
    on.exit(close(handle))
    read.table(handle, sep = sep, header = header)
  }
}
